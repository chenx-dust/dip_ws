<?xml version="1.0"?>
<launch>
    <!-- Launch the ground detector node -->
    <node pkg="ground_detector" type="ground_detector" name="ground_detector" output="screen" />

    <!-- Launch the map registration node -->
    <node pkg="map_registration" type="map_registration" name="map_registration" output="screen" />

    <!-- Launch the decision node -->
    <node pkg="decision" type="decision" name="decision" output="screen" />

    <!-- Launch the detect node -->
    <node pkg="detect" type="detect_template" name="template_matching_node" output="screen" />

    <!-- Optional: Launch RViz for visualization -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find dip_bring_up)/rviz/ground_detector.rviz" respawn="false"/>

</launch>
